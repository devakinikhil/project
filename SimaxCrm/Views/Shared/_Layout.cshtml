<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>@ViewData["Title"]</title>

    <!-- ICONS-->
    <link href="~/lib/@@coreui/icons/css/coreui-icons.min.css" rel="stylesheet">
    <link href="~/lib/flag-icon-css/css/flag-icon.min.css" rel="stylesheet">
    <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/lib/simple-line-icons/css/simple-line-icons.css" rel="stylesheet">
    <!-- /ICONS-->
    
    <!-- TEMPLATE-STYLES -->
    <link href="~/css/style.css" rel="stylesheet">
    <link href="~/vendors/pace-progress/css/pace.css" rel="stylesheet">
    <link href="~/vendors/fastselect-master/dist/fastselect.min.css" rel="stylesheet">
    <!-- /TEMPLATE-STYLES -->
    
    <!-- DATATABLE-STYLES -->
    <link href="~/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.23/css/jquery.dataTables.min.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/buttons/1.6.5/css/buttons.dataTables.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.1/css/bootstrap-select.css" />
    <!-- /DATATABLE-STYLES -->

    <!-- STYLES-TWEAKS -->
    <link rel="stylesheet" href="~/css/site.css">
    <!-- /STYLES-TWEAKS -->
</head>
<body class="app header-fixed sidebar-fixed aside-menu-fixed sidebar-lg-show bg-gray-200">

    <!-- APP-HEADER-->
    <partial name="_app-header" />
    <!-- /APP-HEADER-->

    <!-- ACCEPT COOKIE-->
    <partial name="_CookieConsentPartial" />
    <!-- /ACCEPT COOKIE-->

    <div class="app-body">
        <div class="sidebar">

            <!-- SIDEBAR-NAV -->
            <partial name="_sidebar-nav" />
            <!-- /SIDEBAR-NAV -->

            <button class="sidebar-minimizer brand-minimizer" type="button"></button>
        </div>
        <main class="main">

            <!-- VIEW-BREADCRUMBS -->
            @RenderSection("Breadcrumbs", required: false)
            <!-- /VIEW-BREADCRUMBS -->

            <div class="container-fluid">

                <!-- VIEW-STYLES -->
                @RenderSection("Styles", required: false)
                <!-- /VIEW-STYLES -->

                <div class="animated fadeIn">

                    <!-- VIEW-CONTENT -->
                    @RenderBody()
                    <!-- /VIEW-CONTENT -->

                </div>

                <!-- VIEW-MODALS -->
                @RenderSection("Modals", required: false)
                <!-- /VIEW-MODALS -->

            </div>
        </main>

        <!-- ASIDE-MENU -->
        <partial name="_aside-menu" />
        <!-- /ASIDE-MENU -->
        <!-- POPUP -->
        <div class="modal fade" id="invoiceModalPopup" tabindex="-1" role="dialog" aria-labelledby="invoiceModalPopup" aria-hidden="true" data-keyboard="false" data-backdrop="static">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title">Invoice</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <iframe id="invoice-frame"></iframe>
                    </div>
                </div>
            </div>
        </div>
        <!-- /POPUP -->


    </div>

    <!-- FOOTER -->
    <footer class="app-footer">
        <div>
            <a href="#">InsightCrm</a>
            <span>&copy; 2020 InsightCrm</span>
        </div>
        <div class="ml-auto">
            <span>Powered by</span>
            <a href="#">InsightCrm</a>
        </div>
    </footer>
    <!-- /FOOTER -->

    <!-- VENDOR-SCRIPTS-->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/lib/pace-progress/pace.min.js"></script>
    <script src="~/lib/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>
    <script src="~/lib/@@coreui/coreui/dist/js/coreui.min.js"></script>
    <script src="~/vendors/ckeditor/ckeditor.js"></script>
    <script src="~/vendors/fastselect-master/dist/fastselect.standalone.min.js"></script>
    <script src="~/lib/signalr/dist/browser/signalr.js"></script>
    <!-- /VENDOR-SCRIPTS-->

    <!-- DATATABLE-SCRIPTS-->
    <script type="text/javascript" src="https://cdn.datatables.net/v/bs/dt-1.10.23/datatables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.23/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.5/js/dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.5/js/buttons.html5.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.5/js/buttons.print.min.js"></script>
    <!-- /DATATABLE SCRIPTS-->

    <!-- MAIN-SITE-SCRIPTS-->
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/admin-script.js" asp-append-version="true"></script>
    <script src="~/js/app-main.js"></script>
    <!-- /MAIN-SITE-SCRIPTS-->

    <!-- VIEW-SCRIPTS -->
    @RenderSection("Scripts", required: false)
    <!-- /VIEW-SCRIPTS -->

    <script>
        $(document).ready(function () {
            $('.multipleSelect').fastselect();
            $('.data-table-cl').DataTable({
                dom: 'Bfrtip',
                buttons: [
                    'copy', 'csv', 'excel', 'pdf', 'print'
                ]
            });
        });
    </script>
</body>

</html>